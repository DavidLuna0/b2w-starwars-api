{
    "family": "MyTaskDef",
    "portMappings": [
        {
          "hostPort": 8081,
          "protocol": "tcp",
          "containerPort": 8081
        }
      ],
    "containerDefinitions": [
      {
        "name": "MyTaskDef",
        "image": "922924176525.dkr.ecr.us-east-2.amazonaws.com/starwars-api:latest",
        "cpu": 0,
        "memory": 256,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-create-group": "true",
            "awslogs-group": "/ecs/MyTaskDef",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  